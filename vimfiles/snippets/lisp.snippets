snippet d 函数定义
	defun ${1} (${2})${3}
snippet dv 定义全局变量
	defvar ${1}
# 控制结构
snippet if if
	if (${1}) (${2})
snippet lo 循环
	loop (${1})${2}
snippet li 列表
	list ${1}
snippet dl 列表循环
	dolist (${1})${2}
# 单个关键字
snippet re 返回
	return${1}
snippet la 匿名函数
	lambda ${1}
# 输入输出
snippet p 输出
	print ${1}
snippet fm 格式化输出
	format ${1}
# 常用函数
snippet f_o force-output
	force-output ${1}
snippet r_l read-line
	read-line ${1}
snippet p_i parse-integer
	parse-integer ${1}
snippet y_onp y-or-n-p
	y-or-n-p ${1}
snippet w_of
	with-open-file (${1})${2}
snippet w_sis
	with-standard-io-syntax ${1}
snippet r_in 筛选
	remove-if-not ${1}
snippet r_p
	ripped-p ${1}
